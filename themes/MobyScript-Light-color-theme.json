{
    "name": "MobyScript Light",
    "type": "light",
        "colors": {
            "activityBar.background": "#636161",
            "activityBarBadge.background": "#007acc",
            "activityBarBadge.foreground": "#ffffff",
            "statusBarItem.prominentBackground": "#007acc",
            "activityBar.border": "#8b0303",
            "sideBar.background": "#ffffff",
            "sideBar.border": "#7a0101",
            "menu.background": "#ffffff",
            "tab.border": "#f5f5f5",
            "tab.activeBorder": "#850a0a",
            "tab.activeBackground": "#e0e0e0",
            "tab.inactiveBackground": "#b4b3b3",
            "titleBar.activeBackground": "#ffffff",
            "editor.background": "#ffffff",
            "editorGroupHeader.tabsBackground": "#f5f5f5",
            "statusBar.background": "#f5f5f5",
            "tab.hoverBackground": "#cccccc",
            "editorGroupHeader.tabsBorder": "#e0e0e0",
            "editor.lineHighlightBorder": "#f0f0f0",
            "sideBarSectionHeader.background": "#f5f5f5",
            "sideBarSectionHeader.foreground": "#000000",
            "editorIndentGuide.background1": "#750909",
            "editorIndentGuide.activeBackground1": "#f70000",
            "statusBar.border": "#e0e0e0",
            "tree.indentGuidesStroke": "#cccccc"
        },
        "tokenColors": [
            {
                "scope": [
                    "meta.embedded",
                    "source.groovy.embedded"
                ],
                "settings": {
                    "foreground": "#333333"
                }
            },
            {
                "scope": [
                    "",
                    "source.groovy.embedded"
                ],
                "settings": {
                    "foreground": "#666666"
                }
            },
            {
                "scope": "emphasis",
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "strong",
                "settings": {
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "header",
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#6a9955"
                }
            },
            {
                "scope": "constant.language",
                "settings": {
                    "foreground": "#569cd6"
                }
            },
            {
                "scope": [
                    "constant.numeric"
                ],
                "settings": {
                    "foreground": "#b5cea8"
                }
            },
            {
                "scope": "constant.regexp",
                "settings": {
                    "foreground": "#646695"
                }
            },
            {
                "scope": "entity.name.tag",
                "settings": {
                    "foreground": "#800000"
                }
            },
            {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#795e26"
                }
            },
            {
                "scope": "variable.parameter",
                "settings": {
                    "foreground": "#795e26"
                }
            },
            {
                "scope": "variable.other.readwrite",
                "settings": {
                    "foreground": "#af9804"
                }
            },
            {
                "scope": "entity.name.tag.css",
                "settings": {
                    "foreground": "#d16969"
                }
            },
            {
                "scope": "entity.other.attribute-name",
                "settings": {
                    "foreground": "#795e26"
                }
            },
            {
                "scope": [
                    "entity.other.attribute-name.class.css",
                    "entity.other.attribute-name.class.mixin.css",
                    "entity.other.attribute-name.id.css",
                    "entity.other.attribute-name.parent-selector.css",
                    "entity.other.attribute-name.pseudo-class.css",
                    "entity.other.attribute-name.pseudo-element.css",
                    "source.css.less entity.other.attribute-name.id",
                    "entity.other.attribute-name.attribute.scss",
                    "entity.other.attribute-name.scss"
                ],
                "settings": {
                    "foreground": "#795e26"
                }
            },
            {
                "scope": "invalid",
                "settings": {
                    "foreground": "#d16969"
                }
            },
            {
                "scope": "markup.underline",
                "settings": {
                    "fontStyle": "underline"
                }
            },
            {
                "scope": "markup.bold",
                "settings": {
                    "fontStyle": "bold",
                    "foreground": "#000080"
                }
            },
            {
                "scope": "markup.heading",
                "settings": {
                    "fontStyle": "bold",
                    "foreground": "#000080"
                }
            },
            {
                "scope": "markup.italic",
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": "markup.inserted",
                "settings": {
                    "foreground": "#6a9955"
                }
            },
            {
                "scope": "markup.deleted",
                "settings": {
                    "foreground": "#b22222"
                }
            },
            {
                "scope": "markup.changed",
                "settings": {
                    "foreground": "#569cd6"
                }
            },
            {
                "scope": "punctuation.definition.quote.begin.markdown",
                "settings": {
                    "foreground": "#6a9955"
                }
            },
            {
                "scope": "punctuation.definition.list.begin.markdown",
                "settings": {
                    "foreground": "#d16969"
                }
            },
            {
                "scope": "markup.inline.raw",
                "settings": {
                    "foreground": "#d16969"
                }
            },
            {
                "name": "brackets of XML/HTML tags",
                "scope": "punctuation.definition.tag",
                "settings": {
                    "foreground": "#d16969"
                }
            },
            {
                "scope": "meta.preprocessor",
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "meta.preprocessor.string",
                "settings": {
                    "foreground": "#795e26"
                }
            },
            {
                "scope": "meta.preprocessor.numeric",
                "settings": {
                    "foreground": "#b5cea8"
                }
            },
            {
                "scope": "meta.structure.dictionary.key.python",
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "meta.diff.header",
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "storage",
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "storage.type",
                "settings": {
                    "foreground": "#800000"
                }
            },
            {
                "scope": "storage.modifier",
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "string",
                "settings": {
                    "foreground": "#d16969"
                }
            },
            {
                "scope": "string.tag",
                "settings": {
                    "foreground": "#b22222"
                }
            },
            {
                "scope": "string.value",
                "settings": {
                    "foreground": "#d16969"
                }
            },
            {
                "scope": "string.regexp",
                "settings": {
                    "foreground": "#646695"
                }
            },
            {
                "name": "String interpolation",
                "scope": [
                    "punctuation.definition.template-expression.begin",
                    "punctuation.definition.template-expression.end",
                    "punctuation.section.embedded"
                ],
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "name": "Reset JavaScript string interpolation expression",
                "scope": [
                    "meta.template.expression"
                ],
                "settings": {
                    "foreground": "#333333"
                }
            },
            {
                "scope": [
                    "support.type.vendored.property-name",
                    "support.type.property-name",
                    "variable.css",
                    "variable.scss",
                    "variable.other.less",
                    "source.coffee.embedded"
                ],
                "settings": {
                    "foreground": "#d45555"
                }
            },
            {
                "scope": "keyword",
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "keyword.control",
                "settings": {
                    "foreground": "#800000"
                }
            },
            {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#d16969"
                }
            },
            {
                "scope": [
                    "keyword.operator.new",
                    "keyword.operator.expression",
                    "keyword.operator.cast",
                    "keyword.operator.sizeof",
                    "keyword.operator.instanceof",
                    "keyword.operator.logical.python"
                ],
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "keyword.other.unit",
                "settings": {
                    "foreground": "#b5cea8"
                }
            },
            {
                "scope": [
                    "punctuation.section.embedded.begin.php",
                    "punctuation.section.embedded.end.php"
                ],
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "support.function.git-rebase",
                "settings": {
                    "foreground": "#000080"
                }
            },
            {
                "scope": "constant.sha.git-rebase",
                "settings": {
                    "foreground": "#b5cea8"
                }
            },
            {
                "name": "coloring of the Java import and package identifiers",
                "scope": [
                    "storage.modifier.import.java",
                    "variable.language.wildcard.java",
                    "storage.modifier.package.java"
                ],
                "settings": {
                    "foreground": "#333333"
                }
            },
            {
                "name": "this.self",
                "scope": "variable.language",
                "settings": {
                    "foreground": "#000080"
                }
            }
        ]
    }
    